{
  "name": "root",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "rocket start",
    "build": "turbo run build --filter=./packages/*",
    "dev": "turbo run dev --filter=./packages/*",
    "prettier": "prettier --write packages/**/*.js",
    "docs": "rocket build && npm run github-pages-init",
    "github-pages-init": "echo 'web-widget.js.org'>_site/CNAME && echo ''>_site/.nojekyll"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web-widget/web-widget.git"
  },
  "author": "aui",
  "bugs": {
    "url": "https://github.com/web-widget/web-widget/issues"
  },
  "homepage": "https://github.com/web-widget/web-widget#readme",
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.0",
    "@rocket/cli": "^0.10.2",
    "@rocket/core": "^0.1.2",
    "@rocket/launch": "^0.6.0",
    "@rocket/search": "^0.6.0",
    "alex": "^10.0.0",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "turbo": "^1.10.7"
  },
  "packageManager": "pnpm@7.9.5",
  "pnpm": {
    "patchedDependencies": {},
    "overrides": {
      "typescript": "^5.0.4",
      "tsup": "^7.0.0"
    }
  }
}