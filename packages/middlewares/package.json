      "default": "./dist/inline-styles.js"
    },
    "./powered-by": {
      "types": "./dist/powered-by.d.ts",
      "development": "./src/powered-by.ts",
      "default": "./dist/powered-by.js"
    },
    "./timing": {
      "types": "./dist/timing.d.ts",
      "development": "./src/timing.ts",
      "default": "./dist/timing.js"
