{
  "name": "@web-widget/react",
  "version": "0.0.11",
  "type": "module",
  "exports": {
    ".": {
      "worker": {
        "types": "./dist/react.server.d.ts",
        "development": "./src/server.ts",
        "default": "./dist/react.server.js"
      },
      "browser": {
        "types": "./dist/react.client.d.ts",
        "development": "./src/client.ts",
        "default": "./dist/react.client.js"
      },
      "default": {
        "types": "./dist/react.server.d.ts",
        "development": "./src/server.ts",
        "default": "./dist/react.server.js"
      }
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "development": "./src/vite.ts",
      "default": "./dist/vite.js"
    },
    "./globals": "./globals.d.ts"
  },
  "files": [
    "dist",
    "globals.d.ts"
  ],
  "scripts": {
    "build": "pnpm exec tsup",
    "dev": "pnpm exec tsup --watch",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@web-widget/builder": "workspace:*",
    "@web-widget/schema": "workspace:*",
    "@web-widget/web-widget": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.2.9",
    "@types/react-dom": "^18.0.6",
    "tsup": "^7.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.4.9"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "worker": {
          "types": "./dist/react.server.d.ts",
          "default": "./dist/react.server.js"
        },
        "browser": {
          "types": "./dist/react.client.d.ts",
          "default": "./dist/react.client.js"
        },
        "default": {
          "types": "./dist/react.server.d.ts",
          "default": "./dist/react.server.js"
        }
      },
      "./vite": {
        "types": "./dist/vite.d.ts",
        "default": "./dist/vite.js"
      },
      "./globals": "./globals.d.ts"
    }
  }
}
