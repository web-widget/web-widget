  "devDependencies": {
    "@rollup/plugin-replace": "^3.0.0",
    "@web-sandbox.js/rollup-config": "^0.0.5",
    "@web-sandbox.js/web-widget": "^0.0.5",
    "@web/dev-server": "^0.1.17",
    "@web/dev-server-rollup": "^0.3.11",
    "rollup": "^2.50.5"
  },
