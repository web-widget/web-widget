{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable",
      "WebWorker"
    ],
    "module": "esnext",
    "moduleResolution": "Bundler",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./",
    "rootDir": "./src/",
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
  },
  "include": [
    "schema.d.ts"
  ],
  "exclude": []
}