{
  "plugins": [
    [
      "template-html-minifier",
      {
        "modules": {
          "@web-widget/html": ["html", "unsafeHTML"]
        },
        "strictCSS": false,
        "htmlMinifier": {
          "collapseWhitespace": true,
          "conservativeCollapse": true,
          "removeComments": true,
          "caseSensitive": true,
          "minifyCSS": false
        }
      }
    ]
  ]
}
