{
  "name": "@web-widget/web-router",
  "version": "0.0.3",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/web-router.server.d.ts",
      "default": "./dist/web-router.server.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm exec tsup && pnpm run html-minifier",
