    "lint": "eslint ."
  },
  "dependencies": {
    "@web-widget/html": "workspace:*",
    "@web-widget/schema": "workspace:*"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.9",
    "@web-widget/eslint-config": "workspace:*",
    "babel-plugin-template-html-minifier": "^4.1.0",
    "tsup": "^7.0.0",
    "typescript": "^5.0.4",
    "urlpattern-polyfill": "^9.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
