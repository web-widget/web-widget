{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "ES2015",
    "module": "ES2020",
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "esModuleInterop": true,
    "allowJs": false,
    "strict": true,
    "strictNullChecks": true,
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "declaration": true,
    "declarationMap": false,
    "stripInternal": true,
    "allowSyntheticDefaultImports": false,
    "forceConsistentCasingInFileNames": true,
    "removeComments": false
  },
  "include": [
    "./src"
  ],
  "exclude": []
}